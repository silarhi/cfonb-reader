<!DOCTYPE html>
<html class="h-100">
<head>
    <meta charset="UTF-8">
    <title>Cfonb Reader - {% block title %}{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body class="layout-default">
    <header>
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
            <div class="container">
                <a class="navbar-brand" href="{{ path('app_home') }}">
                    <img
                            title="SILARI"
                            alt="SILARHI"
                            src="{{ asset('build/images/logo-40.png') }}"
                            srcset="{{ asset('build/images/logo-40.png') }} 1x, {{ asset('build/images/logo-80.png') }} 2x"
                            width="40"
                            height="40"
                    >
                    CFONB Reader
                </a>
            </div>
        </nav>
    </header>
    {% block body %}{% endblock %}
</body>
</html>
